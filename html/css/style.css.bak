  /* Jobs Modal Styles */
  
  .jobs-modal-table-container {
    max-height: 400px;
    overflow-y: auto;
    /* Themed scrollbar */
    scrollbar-width: thin;
    scrollbar-color: #444 #222;
  }
  
  .jobs-modal-table-container::-webkit-scrollbar {
    width: 8px;
    background: var(--dark-card, #222);
    border-radius: 8px;
  }
  .jobs-modal-table-container::-webkit-scrollbar-thumb {
    background: var(--border-color, #444);
    border-radius: 8px;
  }
  .jobs-modal-table-container::-webkit-scrollbar-thumb:hover {
    background: var(--tab-active, #666);
  }
  
  .jobs-table-wrapper {
      width: 100%;
      overflow-x: auto;
      margin-top: 16px;
  }
  
  .jobs-donor-table {
      width: 100%;
      border-collapse: separate;
      border-spacing: 0;
      background: rgba(20, 28, 44, 0.98);
      border-radius: 14px;
      box-shadow: 0 4px 24px rgba(0,0,0,0.18);
      font-family: 'Outfit', 'Montserrat', sans-serif;
      color: var(--text-color);
      margin-bottom: 10px;
      overflow: hidden;
  }
  
  .jobs-donor-table {
      width: 100%;
      border-collapse: separate;
      border-spacing: 0;
      background: rgba(20, 28, 44, 0.98);
      border-radius: 14px;
      box-shadow: 0 4px 24px rgba(0,0,0,0.18);
      font-family: 'Outfit', 'Montserrat', sans-serif;
      color: var(--text-color);
      margin-bottom: 10px;
      overflow: hidden;
  }
  .jobs-donor-table th, .jobs-donor-table td {
      padding: 14px 10px;
      text-align: center;
      vertical-align: middle;
      min-width: 110px;
  }
  .jobs-donor-table th {
      position: sticky;
      top: 0;
      z-index: 2;
      background: linear-gradient(90deg, var(--primary-color), var(--accent-color));
      box-shadow: 0 2px 8px rgba(0,0,0,0.10);
  
      background: linear-gradient(90deg, var(--primary-color), var(--accent-color));
      color: #fff;
      font-size: 1.08rem;
      font-weight: 700;
      border-top-left-radius: 10px;
      letter-spacing: 0.5px;
      border-bottom: 2.5px solid var(--border-color, rgba(255,255,255,0.08));
  }
  .jobs-donor-table td {
      background: rgba(30, 40, 60, 0.92);
      font-size: 1rem;
      border-bottom: 1.5px solid var(--border-color, rgba(255,255,255,0.08));
      border-right: 1px solid var(--border-color, rgba(255,255,255,0.04));
      transition: background 0.2s;
  }
  .jobs-donor-table tr:last-child td {
      border-bottom: none;
  }
  .jobs-donor-table tr:hover td {
      background: rgba(50, 70, 120, 0.18);
  }
  .jobs-donor-table td:first-child {
      font-weight: 600;
      color: var(--primary-color);
      background: rgba(var(--primary-color-rgb, 30, 136, 229), 0.10);
      border-left: 2.5px solid var(--primary-color);
      border-radius: 10px 0 0 10px;
  }
  .jobs-donor-table th:first-child {
      border-radius: 10px 0 0 0;
  }
  .jobs-donor-table th:last-child {
      border-radius: 0 10px 0 0;
  }
  .jobs-donor-table td:last-child {
      border-right: none;
      border-radius: 0 10px 10px 0;
  }
  .job-access.yes {
      color: var(--theme-success, #2ecc71);
      font-weight: bold;
      font-size: 1.1em;
  }
  .theme-green {
      color: var(--theme-success, #2ecc71) !important;
      font-weight: bold;
  }
  .theme-yellow {
      color: var(--theme-warning, #ffe066) !important;
      font-weight: bold;
  }
  .theme-red {
      color: var(--theme-danger, #ff4d4f) !important;
      font-weight: bold;
      letter-spacing: 0.01em;
  }
  
  .job-access .pay {
      color: var(--theme-gold, #ffd700);
      font-weight: 700;
      font-size: 1.06em;
      letter-spacing: 0.01em;
  }
  .job-access .perk {
      color: var(--text-secondary, #b3c6e0);
      font-style: italic;
      font-size: 0.96em;
      font-weight: 400;
      letter-spacing: 0.01em;
  }
  .job-access .coming-soon, .job-access.no {
      color: var(--theme-danger, #ff6f61);
      font-weight: bold;
      font-size: 1.08em;
      opacity: 1;
  }
  
  .theme-green {
      color: #2ecc71;
      font-weight: bold;
      font-size: 1.1em;
  }
  
  .theme-red {
      color: #e74c3c;
      font-size: 1.2em;
      opacity: 0.7;
  }
  
  .jobs-donor-table .perk {
      display: block;
      font-size: 0.97em;
      color: var(--accent-color);
      margin-top: 2px;
      font-weight: 500;
  }
  @media (max-width: 700px) {
      .jobs-donor-table th, .jobs-donor-table td {
          min-width: 80px;
          padding: 10px 4px;
          font-size: 0.98em;
      }
      .jobs-modal-content {
          padding: 18px 4px 12px 4px;
      }
  }
  
  .jobs-modal {
      display: none;
      position: fixed;
      z-index: 10000;
      left: 0;
      top: 0;
      width: 100vw;
      height: 100vh;
      background: rgba(10, 14, 25, 0.85);
      backdrop-filter: blur(6px);
      align-items: center;
      justify-content: center;
      transition: opacity 0.3s;
  }
  .jobs-modal.open {
      display: flex;
      animation: fadeIn 0.25s;
  }
  @keyframes fadeIn {
      from { opacity: 0; }
      to { opacity: 1; }
  }
  .jobs-modal-content {
      background: var(--dark-card, rgba(16,23,41,0.92));
      border-radius: 20px;
      box-shadow: 0 8px 32px rgba(0,0,0,0.38);
      padding: 32px 28px 24px 28px;
      width: 838px;
      max-height: 90vh;
      min-height: 600px; /* Increased height for all modals */
      color: var(--text-color);
      position: relative;
      overflow-y: auto;
      border: 1.5px solid var(--border-color, rgba(255,255,255,0.08));
  }
  
  
  
  
  .jobs-modal-header {
      display: flex;
      flex-direction: column;
      align-items: flex-start;
      margin-bottom: 18px;
  }
  .jobs-modal-header h2 {
      font-family: 'Outfit', 'Montserrat', sans-serif;
      font-size: 2rem;
      font-weight: 700;
      margin: 0 0 6px 0;
      color: var(--primary-color);
      letter-spacing: 0.5px;
      display: flex;
      align-items: center;
      gap: 10px;
  }
  .jobs-modal-header .line-decoration {
      width: 70px;
      height: 3px;
      background: linear-gradient(90deg, var(--primary-color), var(--accent-color));
      border-radius: 2px;
      margin-top: 4px;
  }
  .jobs-modal-close {
      position: absolute;
      top: 18px;
      right: 18px;
      background: rgba(0,0,0,0.13);
      border: none;
      color: var(--text-color);
      font-size: 1.5rem;
      border-radius: 50%;
      width: 36px;
      height: 36px;
      cursor: pointer;
      transition: background 0.2s, color 0.2s;
      z-index: 1;
      display: flex;
      align-items: center;
      justify-content: center;
  }
  .jobs-modal-close:hover {
      background: var(--primary-color);
      color: #fff;
  }
  .jobs-modal-sections {
      display: flex;
      flex-wrap: wrap;
      gap: 36px;
      margin-top: 12px;
  }
  .jobs-section, .donor-section {
      flex: 1 1 220px;
      min-width: 220px;
  }
  .jobs-section h3, .donor-section h3 {
      font-size: 1.18rem;
      font-weight: 600;
      color: var(--accent-color);
      margin-bottom: 10px;
      letter-spacing: 0.3px;
  }
  .jobs-list, .donor-list {
      background: rgba(30,136,229,0.07);
      border-radius: 10px;
      padding: 14px 12px;
      min-height: 60px;
      box-shadow: 0 2px 8px rgba(0,0,0,0.10);
  }
  
  /* Staff/Donor Grid Styling */
  .staff-grid, .donor-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
      gap: 20px;
      margin: 20px 0;
  }

  
  /* Donor cards specific styling */
  .donor-grid .staff-card {
      border-top: 3px solid transparent;
      min-height: auto;
      height: 160px;
      padding: 8px;
      padding-bottom: 22px;
      margin-bottom: 8px;
      max-width: 100%;
      position: relative;
      overflow: hidden;
  }
  
  /* Donor tier specific styles */
  .staff-card.diamond {
      border-top-color: #65f9ff;
      box-shadow: 0 3px 12px rgba(101, 249, 255, 0.2);
  }
  
  .staff-card.gold {
      border-top-color: #ffd700;
      box-shadow: 0 3px 12px rgba(255, 215, 0, 0.2);
  }
  
  .staff-card.silver {
      border-top-color: #c0c0c0;
      box-shadow: 0 3px 12px rgba(192, 192, 192, 0.2);
  }
  
  .staff-card.bronze {
      border-top-color: #cd7f32;
      box-shadow: 0 3px 12px rgba(205, 127, 50, 0.2);
  }
  .staff-card:hover {
      transform: translateY(-5px);
  }
  .staff-avatar {
      position: relative;
      width: 70px;
      height: 70px;
      margin: 0 auto 15px;
      border-radius: 50%;
      overflow: hidden;
      border: 3px solid var(--accent-color);
      background-color: rgba(var(--background-color-rgb), 0.1);
      box-shadow: 0 3px 10px rgba(0, 0, 0, 0.2);
  }
  
  .staff-avatar .avatar-background {
      width: 100%;
      height: 100%;
      background-size: cover;
      background-position: center;
      background-color: #333;
      border: 1px solid var(--border-color);
  }
  .staff-avatar .avatar-background {
      width: 100%;
      height: 100%;
      background-size: cover;
      background-position: center;
  }
  .staff-status {
      position: absolute;
      bottom: 5px;
      right: 5px;
      width: 12px;
      height: 12px;
      border: 2px solid #fff;
      border-radius: 50%;
  }
  .staff-status.online { background-color: #2ecc71; }
  .staff-status.away { background-color: #f1c40f; }
  .staff-status.offline { background-color: #e74c3c; }
  .staff-info { margin-bottom: 10px; }
  .staff-name { font-size: 1rem; font-weight: 600; color: var(--text-color); }
  .staff-role { font-size: 0.9rem; color: var(--accent-color); }
  
  .placeholder {
      color: var(--text-secondary);
      font-style: italic;
      padding: 10px 0;
      text-align: center;
  }
  
  .logo-animated {
      animation: logoIntroAnim 4s cubic-bezier(0.77,0,0.175,1) 1 !important;
      animation-fill-mode: forwards !important;
  }
  @keyframes logoIntroAnim {
      0% {
          transform: scale(1.1) rotate(0deg);
          z-index: auto;
          position: static;
          left: auto; top: auto;
      }
      40% {
          transform: scale(0.85) rotate(720deg); /* spin twice, zoom out */
          z-index: auto;
          position: static;
          left: auto; top: auto;
      }
      60% {
          transform: scale(5) rotate(720deg); /* zoom in (500%), stop spinning */
          z-index: 9999;
          position: fixed;
          left: 50vw;
          top: 50vh;
          translate: -50% -50%;
          pointer-events: none;
      }
      85% {
          transform: scale(5) rotate(720deg); /* hold zoomed in */
          z-index: 9999;
          position: fixed;
          left: 50vw;
          top: 50vh;
          translate: -50% -50%;
          pointer-events: none;
      }
      99.9% {
          z-index: 9999;
          position: fixed;
          left: 50vw;
          top: 50vh;
          translate: -50% -50%;
          pointer-events: none;
      }
      100% {
          transform: scale(1) rotate(720deg); /* back to normal size, no spin */
          z-index: auto;
          position: static;
          left: auto; top: auto;
          translate: 0 0;
      }
  }
  
  /* Reset and base styles */
  * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
  }
  
  :root {
      --primary-color: rgb(30, 136, 229);
  
      --dark-bg: rgba(10, 14, 25, 0.85);
      --dark-card: rgba(16, 23, 41, 0.8);
      --text-color: #ffffff;
      --text-secondary: rgba(255, 255, 255, 0.7);
      --border-color: rgba(255, 255, 255, 0.1);
      --glass-effect: 0 8px 32px rgba(0, 0, 0, 0.3);
      --transition-normal: all 0.3s ease;
      
      /* Color variables for roles and tiers */
      --admin-color: #ff5555;
      --developer-color: #55aaff;
      --moderator-color: #55ff7f;
      --helper-color: #ffaa55;
      
      --diamond-color: #55ddff;
      --gold-color: #ffcc44;
      --silver-color: #dddddd;
  }
  
  body {
      font-family: 'Outfit', sans-serif;
      height: 100vh;
      overflow: hidden;
      color: var(--text-color);
      position: relative;
      cursor: default;
  }
  
  /* Accessibility: Forced Colors Mode for high contrast */
  @media (forced-colors: active) {
    body, .stat-card, .server-logo, .feature, .jobs-modal, .jobs-modal-content {
      background: Canvas !important;
      color: CanvasText !important;
      border-color: GrayText !important;
    }
    a, button {
      color: LinkText !important;
      background: ButtonFace !important;
      border-color: ButtonText !important;
    }
    .server-logo {
      filter: none !important;
    }
  }
  
  /* Deprecated -ms-high-contrast rules left for reference */
  /*
  @media screen and (-ms-high-contrast: active) {
    body {
      background: Window !important;
      color: WindowText !important;
    }
  }
  */
  
  /* Background handling */
  #background-container {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: -2;
  }
  
  .background-image {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }
  
  .youtube-container {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      pointer-events: none;
  }
  
  /* YouTube Background Iframe */
  .youtube-background {
      position: absolute;
      top: 50%;
      left: 50%;
      width: 100vw;
      height: 100vh;
      transform: translate(-50%, -50%);
      object-fit: cover;
      pointer-events: none;
      border: none;
      z-index: -2;
  }
  
  /* Fix for different aspect ratios */
  @media (max-aspect-ratio: 16/9) {
      .youtube-background {
          width: 300%;
      }
  }
  
  @media (min-aspect-ratio: 16/9) {
      .youtube-background {
          height: 300%;
      }
  }
  
  /* Custom cursor */
  .custom-cursor {
      position: fixed;
      width: 20px;
      height: 20px;
      pointer-events: none;
      z-index: 9999;
      transform: translate(-50%, -50%);
      will-change: transform;
      transition: transform 0.1s ease;
  }
  
  .custom-cursor::before {
      content: '';
      position: absolute;
      width: 8px;
      height: 8px;
      background-color: #fff;
      border-radius: 50%;
      top: 6px;
      left: 6px;
      transform: translate(0, 0);
      box-shadow: 0 0 10px rgba(var(--primary-color-rgb), 0.8);
  }
  
  .custom-cursor::after {
      content: '';
      position: absolute;
      width: 20px;
      height: 20px;
      border: 2px solid rgba(var(--primary-color-rgb), 0.7);
      border-radius: 50%;
      top: 0;
      left: 0;
      animation: pulse 2s infinite;
  }
  
  .custom-cursor.active::before {
      background-color: rgba(var(--accent-color-rgb), 1);
  }
  
  @keyframes pulse {
      0% { transform: scale(1); opacity: 0.7; }
      50% { transform: scale(1.1); opacity: 1; }
      100% { transform: scale(1); opacity: 0.7; }
  }
  
  /* Background overlay */
  .background-overlay {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(to bottom, rgba(0, 0, 0, 0.6), rgba(0, 0, 0, 0.8));
      z-index: -1;
  }
  
  /* Main container layout */
  .main-container {
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      height: 100vh;
      padding: 1.5rem 4rem;
      max-width: 1800px;
      margin: 0 auto;
      position: relative;
  }
  
  /* LOGO HEADER STYLING */
  header {
      padding-top: 0.5rem;
      display: flex;
      justify-content: center;
      align-items: center;
      margin-bottom: 0.5rem;
      flex-shrink: 0;
  }
  
  .logo-only-container {
      display: flex;
      justify-content: center;
      align-items: center;
      height: 110px;
      padding: 0;
      margin: 0;
  }
  
  .server-logo {
      height: 110px;
      width: auto;
      max-width: 260px;
      object-fit: contain;
      filter: drop-shadow(0 8px 12px rgba(0, 0, 0, 0.5));
      transition: all 0.5s ease;
      transform-origin: center center;
  }
  
  /* Logo animation */
  .logo-animated {
      animation: professionalLogoAnim 6s infinite;
  }
  
  @keyframes professionalLogoAnim {
      0% { 
          transform: translateY(0) scale(1);
          filter: drop-shadow(0 8px 12px rgba(0, 0, 0, 0.5));
      }
      20% { 
          transform: translateY(-5px) scale(1.02);
          filter: drop-shadow(0 10px 15px rgba(0, 0, 0, 0.6)) brightness(1.05);
      }
      40% { 
          transform: translateY(0) scale(1);
          filter: drop-shadow(0 8px 12px rgba(0, 0, 0, 0.5)) brightness(1);
      }
      60% { 
          transform: translateY(-3px) scale(1.01);
          filter: drop-shadow(0 9px 14px rgba(0, 0, 0, 0.55)) brightness(1.03);
      }
      80% { 
          transform: translateY(-2px) scale(1.005);
          filter: drop-shadow(0 9px 13px rgba(var(--primary-color-rgb), 0.2)) brightness(1.02);
      }
      100% { 
          transform: translateY(0) scale(1);
          filter: drop-shadow(0 8px 12px rgba(0, 0, 0, 0.5));
      }
  }
  
  .logo-only-container:hover .server-logo {
      filter: drop-shadow(0 0 15px rgba(var(--primary-color-rgb), 0.7)) brightness(1.15);
      transform: scale(1.03);
  }
  
  /* Content container */
  .content-container {
      display: flex;
      flex-direction: column;
      gap: 1.2rem;
      width: 100%;
      max-width: 1400px;
      margin: 0 auto;
      animation: fadeIn 1.2s ease-out forwards;
      flex: 1;
      overflow: hidden;
  }
  
  /* Categories Tabs */
  .categories-tabs {
      background-color: var(--dark-bg);
      border-radius: 12px;
      padding: 0.75rem;
      box-shadow: var(--glass-effect);
      backdrop-filter: blur(8px);
      border: 1px solid var(--border-color);
      margin-bottom: 0.5rem;
      flex-shrink: 0;
  }
  
  .tabs-container {
      display: flex;
      justify-content: space-between;
      align-items: center;
      gap: 0.5rem;
  }
  
  .tab {
      display: flex;
      align-items: center;
      gap: 0.5rem;
      padding: 0.75rem 1.25rem;
      border-radius: 8px;
      cursor: pointer;
      transition: var(--transition-normal);
      flex: 1;
      justify-content: center;
      background-color: rgba(0, 0, 0, 0.2);
      position: relative;
      overflow: hidden;
      border: 1px solid var(--border-color);
  }
  
  .tab:not(.active):hover {
      background-color: rgba(var(--primary-color-rgb), 0.15);
      transform: translateY(-2px);
      border-color: var(--border-color);
      box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
  }
  
  .tab.active {
      background: linear-gradient(135deg, rgba(var(--primary-color-rgb), 0.8), rgba(var(--accent-color-rgb), 0.8));
      box-shadow: 0 4px 15px rgba(var(--primary-color-rgb), 0.3);
      border-color: var(--border-color);
  }
  
  .tab.active::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: linear-gradient(to bottom right, rgba(255, 255, 255, 0.15), transparent);
      pointer-events: none;
  }
  
  .tab i {
      font-size: 1rem;
  }
  
  .tab span {
      font-weight: 600;
      font-size: 0.9rem;
      letter-spacing: 0.5px;
  }
  
  /* Tab Content Container - FIXED DISPLAY ISSUES */
  .tab-content-container {
      position: relative;
      flex: 1;
      display: flex;
      overflow: hidden;
      margin-bottom: 0.5rem;
      min-height: 0; /* Fix for flexbox overflow */
  }
  
  .tab-content {
      display: none;
      animation: fadeIn 0.5s ease-out forwards;
      width: 100%;
      height: 100%;
      overflow-y: auto; /* Allow scrolling on all tab content */
      /*padding-right: 5px;  Add padding for scrollbar */
  }
  
  /* Hide scrollbar but keep functionality */
  .tab-content::-webkit-scrollbar {
      width: 6px;
  }
  
  .tab-content::-webkit-scrollbar-track {
      background: transparent;
  }
  
  .tab-content::-webkit-scrollbar-thumb {
      background-color: rgba(var(--primary-color-rgb), 0.3);
      border-radius: 6px;
  }
  
  .tab-content.no-scroll {
      overflow-y: auto; /* Changed from hidden to auto to allow scrolling */
  }
  
  .tab-content.active {
      display: block;
  }
  
  /* Common panel styling - FIXED HEIGHT ISSUES */
  .server-updates-panel,
  .server-info-panel,
  .staff-panel,
  .donors-panel,
  .about-panel,
  .minigame-panel {
      background-color: var(--dark-bg);
      border-radius: 12px;
      padding: 1.5rem;
      box-shadow: var(--glass-effect);
      backdrop-filter: blur(8px);
      border: 1px solid var(--border-color);
      margin-bottom: 1rem;
      min-height: min-content; /* Ensure content fits */
      display: flex;
      flex-direction: column;
      position: relative;
      height: auto; /* Changed from fixed height to auto */
  }
  
  .info-header {
      margin-bottom: 1.5rem;
      display: flex;
      flex-direction: column;
      align-items: center;
  }
  
  .info-header h2 {
      font-weight: 600;
      font-size: 1.5rem;
      letter-spacing: 2px;
      margin-bottom: 0.75rem;
  }
  
  .line-decoration {
      width: 80px;
      height: 3px;
      background: linear-gradient(90deg, transparent, var(--border-color, #999), transparent);
  }
  
  /* Stats grid */
  .stats-grid {
      display: grid;
      grid-template-columns: repeat(4, 1fr);
      gap: 1.5rem;
      margin-bottom: 2rem;
  }
  
  .stat-card {
      background-color: var(--dark-card);
      border-radius: 10px;
      padding: 1.25rem;
      display: flex;
      align-items: center;
      border: 1px solid var(--border-color);
      box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);
      transition: var(--transition-normal);
      overflow: hidden;
      position: relative;
  }
  
  .stat-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 6px 20px rgba(var(--primary-color-rgb), 0.2);
      border-color: rgba(var(--primary-color-rgb), 0.3);
  }
  
  .stat-card::before {
      content: "";
      position: absolute;
      top: 0;
      left: -100%;
      width: 100%;
      height: 100%;
      background: linear-gradient(to right, transparent, rgba(255, 255, 255, 0.1), transparent);
      animation: shimmer 3s infinite;
      z-index: 1;
      pointer-events: none;
  }
  
  @keyframes shimmer {
      0% { left: -100%; }
      100% { left: 100%; }
  }
  
  .stat-icon {
      width: 50px;
      height: 50px;
      min-width: 50px;
      background: var(--primary-color);
      border-radius: 10px;
      display: flex;
      justify-content: center;
      align-items: center;
      margin-right: 1rem;
      position: relative;
      overflow: hidden;
  }
  
  .stat-icon::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: linear-gradient(to bottom right, rgba(255, 255, 255, 0.2), transparent);
      border-radius: 10px;
      pointer-events: none;
  }
  
  .stat-icon i {
      font-size: 1.5rem;
      color: white;
      position: relative;
      z-index: 2;
  }
  
  .stat-content {
      flex: 1;
  }
  
  .stat-title {
      font-size: 0.8rem;
      font-weight: 600;
      color: var(--text-secondary);
      letter-spacing: 1px;
      margin-bottom: 0.3rem;
  }
  
  .stat-value {
      font-size: 1.1rem;
      font-weight: 600;
      color: var(--text-color);
  }
  
  /* Server features */
  .server-features {
      display: flex;
      justify-content: space-between;
      flex-wrap: wrap;
      gap: 1rem;
      margin-bottom: 1.5rem;
  }
  
  .feature {
      display: flex;
      align-items: center;
      gap: 0.5rem;
      padding: 0.75rem 1.25rem;
      background-color: rgba(var(--primary-color-rgb), 0.1);
      border-radius: 20px;
      font-size: 0.9rem;
      font-weight: 500;
      transition: var(--transition-normal);
  }
  
  .feature:hover {
      background-color: var(--border-color, #999);
      transform: translateY(-2px);
  }
  
  .feature i {
      color: rgba(var(--accent-color-rgb), 1);
  }
  
  /* Server Updates Styles */
  .updates-container {
      display: flex;
      flex-direction: column;
      gap: 1.5rem;
      max-width: 900px;
      margin: 0 auto;
  }
  
  .update-loader {
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 0.75rem;
      padding: 2rem;
      color: var(--text-secondary);
      font-weight: 500;
      text-align: center;
  }
  
  .loading-spinner {
      position: relative;
      width: 60px;
      height: 60px;
      margin-bottom: 0.5rem;
  }
  
  .loading-spinner i {
      font-size: 2rem;
      color: var(--accent-color);
      animation: spin 1.5s infinite linear;
  }
  
  .loading-spinner.connecting i {
      animation: pulse 1.8s infinite ease-in-out;
      font-size: 2.5rem;
  }
  
  .loading-spinner.error i {
      animation: shake 0.8s ease-in-out;
      color: var(--theme-danger);
  }
  
  @keyframes spin {
      0% { transform: rotate(0deg); }
      100% { transform: rotate(360deg); }
  }
  
  @keyframes pulse {
      0% { transform: scale(0.95); opacity: 0.7; }
      50% { transform: scale(1.05); opacity: 1; }
      100% { transform: scale(0.95); opacity: 0.7; }
  }
  
  @keyframes shake {
      0%, 100% { transform: translateX(0); }
      20%, 60% { transform: translateX(-5px); }
      40%, 80% { transform: translateX(5px); }
  }
  
  .connecting-status {
      margin-top: 0.5rem;
      display: flex;
      align-items: center;
      gap: 0.5rem;
  }
  
  .connecting-dots {
      display: flex;
      gap: 4px;
  }
  
  .connecting-dot {
      width: 6px;
      height: 6px;
      background-color: var(--accent-color);
      border-radius: 50%;
      opacity: 0.4;
  }
  
  .connecting-dot:nth-child(1) {
      animation: dot-fade 1.5s infinite ease-in-out;
      animation-delay: 0s;
  }
  
  .connecting-dot:nth-child(2) {
      animation: dot-fade 1.5s infinite ease-in-out;
      animation-delay: 0.5s;
  }
  
  .connecting-dot:nth-child(3) {
      animation: dot-fade 1.5s infinite ease-in-out;
      animation-delay: 1s;
  }
  
  @keyframes dot-fade {
      0%, 100% { opacity: 0.4; }
      50% { opacity: 1; }
  }
  
   .update-card {
     background-color: var(--dark-card);
     border-radius: 12px;
     display: flex;
