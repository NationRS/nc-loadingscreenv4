  .tip-container {
      display: flex;
      align-items: center;
      background-color: var(--dark-bg);
      border-radius: 12px;
      padding: 1.25rem;
      box-shadow: var(--glass-effect);
      border: 1px solid var(--border-color);
  }
  
  .tip-icon {
      min-width: 40px;
      min-height: 40px;
      background-color: rgba(var(--primary-color-rgb), 0.15);
      color: var(--primary-color);
      display: flex;
      justify-content: center;
      align-items: center;
      margin-right: 1rem;
      font-size: 1.5rem;
      border-radius: 50%;
  }
  
  .tip-content {
      flex: 1;
  }
  
  .tip-title {
      font-weight: 600;
      letter-spacing: 1px;
      margin-bottom: 0.3rem;
      color: var(--accent-color);
  }
  
  .tip-text {
      font-size: 0.95rem;
      color: var(--text-secondary);
      transition: opacity 0.5s ease;
  }
  
  /* THEMED PROGRESS BAR STYLES */
  .progress-container {
      background-color: var(--dark-bg);
      border-radius: 12px;
      padding: 1.5rem;
      box-shadow: var(--glass-effect);
      border: 1px solid var(--border-color);
      position: relative;
      overflow: hidden;
  }
  
  .progress-container::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      height: 1px;
      background: linear-gradient(90deg, transparent, rgba(var(--accent-color-rgb), 0.5), transparent);
      opacity: 0.5;
  }
  
  .loading-status {
      font-size: 1rem;
      margin-bottom: 0.5rem;
      text-align: center;
      font-weight: 500;
      letter-spacing: 0.5px;
      color: rgba(var(--accent-color-rgb), 0.9);
      text-shadow: 0 0 10px rgba(var(--accent-color-rgb), 0.3);
  }
  
  /* GTA V Realistic Scene Animation */
  .gta-scene-container {
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      margin-bottom: 1rem;
      height: 90px;
      position: relative;
      perspective: 800px;
      overflow: hidden;
  }
  
  .gta-realistic-scene {
      position: relative;
      width: 320px;
      height: 85px;
      background: linear-gradient(to bottom, #6ab7ff, #c9e6ff);
      transform-style: preserve-3d;
      transform: perspective(800px) rotateX(10deg);
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.3);
      border-radius: 4px;
      overflow: hidden;
  }
  
  /* City Background */
  .gta-city-bg {
      position: absolute;
      width: 100%;
      height: 40px;
      bottom: 25px;
      left: 0;
      z-index: 1;
  }
  
  .skyscraper {
      position: absolute;
      bottom: 0;
      background: linear-gradient(to bottom, rgba(var(--primary-color-rgb), 0.8), rgba(var(--primary-color-rgb), 0.5));
      border: 1px solid rgba(255, 255, 255, 0.3);
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
  }
  
  .skyscraper.s1 {
      height: 35px;
      width: 18px;
      left: 40px;
      background: linear-gradient(to bottom, #3a3a3a, #1a1a1a);
  }
  
  .skyscraper.s2 {
      height: 45px;
      width: 22px;
      left: 120px;
      background: linear-gradient(to bottom, var(--primary-color), rgba(var(--primary-color-rgb), 0.7));
  }
  
  .skyscraper.s3 {
      height: 30px;
      width: 20px;
      left: 200px;
      background: linear-gradient(to bottom, #555, #333);
  }
  
  /* Road */
  .gta-road {
      position: absolute;
      width: 100%;
      height: 40px;
      bottom: 0;
      background: #333;
      transform: perspective(500px) rotateX(45deg);
      transform-origin: bottom;
      z-index: 2;
  }
  
  .road-lane {
      position: absolute;
      width: 100%;
      height: 2px;
      top: 50%;
      background: linear-gradient(to right, #ffcc00 10px, transparent 10px, transparent 20px, #ffcc00 20px);
      background-size: 30px 2px;
      background-repeat: repeat-x;
      animation: moveLane 3s linear infinite;
  }
  
  @keyframes moveLane {
      0% { background-position: 0 0; }
      100% { background-position: -30px 0; }
  }
  
  /* Sports Car */
  .gta-car {
      position: absolute;
      width: 120px;
      height: 30px;
      bottom: 10px;
      right: 40px;
      z-index: 3;
      animation: carBounce 0.5s infinite alternate;
      transform-style: preserve-3d;
  }
  
  @keyframes carBounce {
      0% { transform: translateY(0); }
      100% { transform: translateY(-1px); }
  }
  
  .car-body {
      position: absolute;
      width: 100%;
      height: 100%;
      bottom: 0;
      background: linear-gradient(to bottom, var(--accent-color), rgba(var(--accent-color-rgb), 0.7));
      border-radius: 5px 15px 1px 5px;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.5);
      z-index: 1;
  }
  
  .car-roof {
      position: absolute;
      width: 40px;
      height: 12px;
      background: rgba(0, 0, 0, 0.8);
      border-radius: 3px;
      top: -10px;
      left: 30px;
      transform-style: preserve-3d;
      transform: rotateX(5deg);
      z-index: 2;
  }
  
  .car-window {
      position: absolute;
      width: 20px;
      height: 8px;
      background: rgba(200, 230, 255, 0.7);
      border: 1px solid rgba(255, 255, 255, 0.2);
      top: -8px;
      z-index: 2;
  }
  
  .car-window.front {
      left: 30px;
      border-radius: 1px 3px 1px 1px;
      transform: skewX(-10deg);
  }
  
  .car-window.rear {
      left: 52px;
      border-radius: 3px 1px 1px 1px;
      transform: skewX(15deg);
  }
  
  .car-headlight {
      position: absolute;
      width: 8px;
      height: 4px;
      background: rgba(255, 255, 255, 0.9);
      border-radius: 2px;
      top: 10px;
      left: 2px;
      box-shadow: 0 0 5px rgba(255, 255, 255, 0.8);
      z-index: 2;
  }
  
  .car-taillight {
      position: absolute;
      width: 5px;
      height: 3px;
      background: rgba(255, 0, 0, 0.8);
      border-radius: 1px;
      top: 10px;
      right: 2px;
      box-shadow: 0 0 5px rgba(255, 0, 0, 0.5);
      z-index: 2;
  }
  
  .car-wheel {
      position: absolute;
      width: 14px;
      height: 14px;
      background: #111;
      border-radius: 50%;
      bottom: -5px;
      border: 2px solid #333;
      z-index: 0;
      animation: wheelSpin 1s linear infinite;
  }
  
  .car-wheel::after {
      content: '';
      position: absolute;
      width: 6px;
      height: 6px;
      background: #444;
      border-radius: 50%;
      top: 4px;
      left: 4px;
  }
  
  @keyframes wheelSpin {
      0% { transform: rotate(0deg); }
      100% { transform: rotate(360deg); }
  }
  
  .car-wheel.front {
      left: 20px;
  }
  
  .car-wheel.rear {
      right: 20px;
  }
  
