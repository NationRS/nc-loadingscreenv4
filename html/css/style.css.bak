  .update-retry-btn:hover {
      border-color: var(--accent-color);
      color: var(--accent-color);
      transform: translateY(-2px);
      box-shadow: 0 4px 12px rgba(var(--accent-color-rgb), 0.2);
  }
  
  .update-retry-btn:active {
