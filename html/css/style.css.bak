/* Define theme variables */
:root {
    /* Benefits section width control - modify these values to adjust width */
    --benefits-width: 100%;  /* Controls overall width of benefits section */
    --benefits-max-width: none;  /* Sets maximum width limit */
  }
  
  /* Simple pulse animation for benefit icons */
  @keyframes pulse-icon {
    0% { transform: scale(1); }
    100% { transform: scale(1.3); }
  }
  @import url('https://fonts.googleapis.com/css2?family=Outfit:wght@300;400;500;600;700&family=Rajdhani:wght@500;600;700&display=swap');
  @import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700;800;900&display=swap');
  
