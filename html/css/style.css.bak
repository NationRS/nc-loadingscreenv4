      box-shadow: 0 2px 4px rgba(var(--accent-color-rgb), 0.1);
  }
  
  /* SERVER INFO ADDITIONAL CONTENT - FIXED LAYOUT */
  .server-highlights {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 1.5rem;
      margin-bottom: 1rem; /* Added margin to prevent content from being cut off */
  }
  
  .highlight-card {
